        {
          "@id": "scs:dist/identity/configuration/IdentityProviderFactory#IdentityProviderFactory_config",
          "range": "rdf:JSON",
          "comment": "JSON config for the OIDC library",
          "unique": true,
          "required": true
        },
        {
          "@id": "scs:dist/identity/configuration/IdentityProviderFactory#IdentityProviderFactory_args_adapterFactory",
          "range": "scs:dist/identity/storage/AdapterFactory#AdapterFactory",
          "comment": "Factory that creates the adapter used for OIDC data storage.",
          "unique": true,
          "required": true
        },
        {
          "@id": "scs:dist/identity/configuration/IdentityProviderFactory#IdentityProviderFactory_args_baseUrl",
          "range": "xsd:string",
          "comment": "Base URL of the server.",
          "unique": true,
          "required": true
        },
        {
          "@id": "scs:dist/identity/configuration/IdentityProviderFactory#IdentityProviderFactory_args_idpPath",
          "range": "xsd:string",
          "comment": "Path of the IDP component in the server. Should start with a slash.",
          "unique": true,
          "required": true
        },
        {
          "@id": "scs:dist/identity/configuration/IdentityProviderFactory#IdentityProviderFactory_args_storage",
          "range": "scs:dist/storage/keyvalue/KeyValueStorage#KeyValueStorage",
          "comment": "Storage used to store cookie and JWT keys so they can be re-used in case of multithreading.",
